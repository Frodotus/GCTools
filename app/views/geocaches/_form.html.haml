= form_for @geocach do |f|
  - if @geocach.errors.any?
    #error_explanation
      %h2= "#{pluralize(@geocach.errors.count, "error")} prohibited this geocach from being saved:"
      %ul
        - @geocach.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :name
    = f.text_field :name
  .field
    = f.label :code
    = f.text_field :code
  .actions
    = f.submit 'Save'
